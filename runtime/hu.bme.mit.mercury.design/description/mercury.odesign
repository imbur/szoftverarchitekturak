                  <subModelOperations xsi:type="tool_1:If" conditionExpression="[target.oclIsTypeOf(ProgramOutputVariable) or target.oclIsTypeOf(OutputVariable)/]">
                    <subModelOperations xsi:type="tool_1:If" conditionExpression="[target.eContainer()= source.eContainer().eContainer()/]">
                      <subModelOperations xsi:type="tool_1:SetValue" featureName="target" valueExpression="[self.oclAsType(DataSource).target->including(target)/]"/>
                    </subModelOperations>
                  </subModelOperations>
                </subModelOperations>
                <subModelOperations xsi:type="tool_1:If" conditionExpression="[source.oclIsTypeOf(InputVariable) or source.oclIsTypeOf(ProgramInputVariable)/]">
                  <subModelOperations xsi:type="tool_1:If" conditionExpression="[target.oclIsTypeOf(FunctionInputVariableReference)/]">
                    <subModelOperations xsi:type="tool_1:If" conditionExpression="[source.eContainer()= target.eContainer().eContainer()/]">
                      <subModelOperations xsi:type="tool_1:SetValue" featureName="target" valueExpression="[self.oclAsType(DataSource).target->including(target)/]"/>
                    </subModelOperations>
                  </subModelOperations>
                  <subModelOperations xsi:type="tool_1:If" conditionExpression="[target.oclIsTypeOf(OutputVariable) or target.oclIsTypeOf(ProgramOutputVariable)/]">
                    <subModelOperations xsi:type="tool_1:If" conditionExpression="[source.eContainer()= target.eContainer()/]">
                      <subModelOperations xsi:type="tool_1:SetValue" featureName="target" valueExpression="[self.oclAsType(DataSource).target->including(target)/]"/>
                    </subModelOperations>
                  </subModelOperations>
                </subModelOperations>
